<!DOCTYPE html>
<html>
  <head>
    <title>users</title>
    <%- include('../partials/head'); %>
  </head>
  <body>
    <header>
      <%- include('../partials/header'); %>
    </header>
    <ul>
      <% users.forEach((user)=> { %>
          <li>
              <h2>
                  <%= user.username %>
              </h2>
              <p>
                  <%= user.email %>
              </p>
          </li>
          <hr />
      <% }) %>
      </ul>
      <footer>
        <%- include('../partials/footer'); %>
      </footer>  
  </body>
</html>