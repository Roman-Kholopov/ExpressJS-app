<html>
  <head>
    <title>Create new user</title>
    <%- include('../partials/head'); %>  </head>
  <body>
    <header>
      <%- include('../partials/header'); %>
    </header>
    <h1><%= title %></h1>
    <% for (key in error) { %>
      <p>Ошибка в поле <%= error[key] %></p>
    <% } %>
    <button onclick="window.location.href='/users/<%= userId %>/edit'"><%= button %></button>
    <footer>
      <%- include('../partials/footer'); %>
    </footer>  
  </body>
</html>
