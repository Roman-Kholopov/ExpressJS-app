<!DOCTYPE html>
<html>
  <head>
    <title>users</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
  <ul>
    <% users.forEach((user)=> { %>
        <li>
            <h2>
                <%= user.name %>
            </h2>
            <p>
                <%= user.id %>
            </p>
            <p>
                <%= user.username %>
            </p>
            <p>
                <%= user.email %>
            </p>
            <p>
                <%= user.phone %>
            </p>
            <p>
                <%= user.website %>
            </p>
        </li>
        <hr />
    <% }) %>
    </ul>
  </body>
</html>